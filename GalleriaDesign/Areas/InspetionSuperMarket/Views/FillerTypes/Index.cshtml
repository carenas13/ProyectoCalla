@model IEnumerable<Supermarket.Models.FillerType>

@{
    ViewBag.Title = "Index";
}

<h2>Filler Type</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nameFiller)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.nameFiller)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idFillerType }) |
            @Html.ActionLink("Details", "Details", new { id=item.idFillerType }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idFillerType })
        </td>
    </tr>
}

</table>
